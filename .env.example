# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLOUD_PROJECT=your_project_id

# Google Search API
GOOGLE_SEARCH_API_KEY=your_search_api_key
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id

# FDA API (optional, has public access)
FDA_API_KEY=your_fda_api_key_optional

# ClinicalTrials.gov API (public, no key needed usually)
CLINICALTRIALS_API_KEY=optional

# Redis for Memory Bank (optional, can use in-memory)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=pharma-agent

# Agent Configuration
LOG_LEVEL=INFO
ENABLE_TRACING=true
ENABLE_METRICS=true

# A2A Protocol
A2A_ENDPOINT=http://localhost:8080
A2A_AUTH_TOKEN=your_a2a_token
